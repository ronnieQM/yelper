#!/home/rqm/dev/yelper/yelpScraper3.6/venv/bin/python3.6
# EASY-INSTALL-ENTRY-SCRIPT: 'OpenCobolIDE==4.7.6','gui_scripts','opencobolide'
__requires__ = 'OpenCobolIDE==4.7.6'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('OpenCobolIDE==4.7.6', 'gui_scripts', 'opencobolide')()
    )
